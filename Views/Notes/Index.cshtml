@model System.Collections.Generic.List<MVCNotesSaver.Models.User.NoteViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<form method="post">
    <input type="text" class="form-text" name="noteText"/>
    <input type="submit" class="form-control" value="Send"/>
</form>

<table>
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Created at</th>
        <th scope="col">Text</th>
    </tr>
    </thead>
    <tbody>
    @if (Model != null)
    {
        foreach (var note in Model)
        {
            <tr>
                <td>@note.Id</td>
                <td>@note.Created</td>
                <td>@note.Text</td>
            </tr>
        }
    }
    </tbody>
</table>